<div class="profile_featured_section" style="margin: 10px 127.5px;">
	<h2><span>Contact Us </span>CONTACT</h2>
		<div style="width: 90%; padding: 2% 10%">
		<%= form_for @feedback do |f| %>
      <%= render :partial => 'shared/errors', :locals => {:target => @feedback } %>
      <div style="float:left;width: 20%;line-height:30px;height:450px">
        <%= f.label :first_name, "First Name: " %><br />
        <%= f.label :last_name, "Last Name: " %><br />
        <%= f.label :email, "Email Address: " %><br />
        <%= f.label :comments, "Comments: " %><br />
      </div>
      <div id="userInfoTblEdit" style="float:right;width:80%;height:100%">
        <%= f.text_field :first_name, :class => 'textField', :style=>"margin-bottom: 5px;" %><br />
        <%= f.text_field :last_name, :class => 'textField', :style=>"margin-bottom: 5px;" %><br />
        <%= f.email_field :email, :class => 'textField', :style=>"margin-bottom: 5px;" %><br />
        <%= f.text_area :comments, :class => 'textField', :style=>"margin-bottom: 5px;" %><br />
      </div>
      <div>

      <div style="float:left;">
        <%= recaptcha_tags %>
      </div>
        <%= f.submit 'Send', :class => "blackInputBtn" %>
      </div>
      <div style="clear:both;" ></div>
		<% end %>
		</div>
</div>
