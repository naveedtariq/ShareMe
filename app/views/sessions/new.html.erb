WAJJO<div class="get_share_code_signin_popup">
  <h2> Sign in to <span>SHARE ME</span></h2>
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <div id="error-explanation"></div>
    <br />

    <label>Email:</label>
    <%= f.email_field :email ,:class=>"textField", :id=>"signin-e" %>
    <label>Password:</label>
    <%= f.password_field :password, :class=>"textField", :id=>"signin-n"%>
    <input type="button" onclick="tb_remove()" value="Cancel" class="blackInputBtnCancel"/>
    <input id="sign-in" type="submit" value="Sign in"  class="blackInputBtn" onclick="javascript:signIn();return false;"/>
  <% end %>

  <div style="padding-left:35px;">
    <ul class="connect_with_socials">
      <p>Or connect with</p>
      <li>
        <%= link_to "",user_omniauth_authorize_path(:facebook),:id=>"connect_with_facebook",:class=>"social-fb tipsy",:title=>"facebook" %>
      </li>
      <li>
        <%= link_to "",user_omniauth_authorize_path(:twitter),:id=>"connect_with_twitter",:class=>"social-twitt tipsy",:title=>"twitter" %>
      </li>
      <li>
        <a class="social-linkedin tipsy" href="#" title="LinkedIn" onclick="social_connect('linked_in');return false;"></a>
        <input type="radio" value="linked_in" name="oauth_provider" id="oauth_provider_linked_in" style="display: none;">
      </li>
    </ul>
    <%# end %>
  </div>
</div>
