<div class="profile_featured_section">
  <h2><span><%= contact.name.capitalize %>'S </span>CONTACT INFORMATION</h2>
  <table width="600" border="0" id="userInfoTbl">
    <tr>
      <td width="200"><strong>First Name</strong></td>
      <td><%= contact.associated_user.f_name %></td>
    </tr>
    <tr>
      <td width="200"><strong>Last Name</strong></td>
      <td><%= contact.associated_user.l_name %></td>
    </tr>
    <tr>
    <td width="200"><strong>Company Name</strong></td>
    <td><%= contact.associated_user.profile.company_name %></td>
    </tr>
    <tr>
    <td width="200"><strong>Company Address</strong></td>
    <td><%= contact.associated_user.profile.company_address %></td>
    </tr>
    <tr>
    <td width="200">&nbsp;</td>
    <td>&nbsp;</td>
    </tr>
    <tr>
    <td width="200"><strong>Email </strong></td>
    <td><%= mail_to contact.email %></td>
    </tr>
    <tr>
    <td width="200"><strong>Phone Number </strong></td>
    <td><a href="callto:<%= contact.associated_user.profile.phone %>"><%= contact.associated_user.profile.phone%></a></td>
    </tr>
    <tr>
    <td width="200"> <strong>Fax</strong></td>
    <td><%= contact.associated_user.profile.fax %></td>
    </tr>
  </table> 
</div>
