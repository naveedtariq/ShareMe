<script type="text/javascript">
  if(<%=@already%>)
  {
    self.opener.location.href = 'http://localhost.com/user_home';
    self.close();
  }
  else if(!<%=@activation%>){
  //  window.opener.show_notification('Error','<%=flash[:error]%>','error');
    window.opener.show_no_confirmation();
    self.close();
  }
  else{
    if(<%= @dd%>)
    {
      window.opener.show_social_form();
    }
    window.opener.oauth_data("facebook");
    self.close();
  }
</script>
